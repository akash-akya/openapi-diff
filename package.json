{
  "name": "openapi-diff",
  "version": "0.13.1",
  "description": "A CLI tool to identify differences between Swagger/OpenAPI specs.",
  "bin": {
    "openapi-diff": "./bin/openapi-diff"
  },
  "main": "dist/api.js",
  "scripts": {
    "release-major": "./node_modules/.bin/gulp release --type major",
    "release-minor": "./node_modules/.bin/gulp release --type minor",
    "release-patch": "./node_modules/.bin/gulp release --type patch",
    "test": "./node_modules/.bin/gulp",
    "watch": "./node_modules/.bin/gulp watch",
    "watch-e2e": "./node_modules/.bin/gulp watch-e2e"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org/atlassian/openapi-diff.git"
  },
  "keywords": [
    "diff",
    "difference",
    "OpenAPI",
    "Swagger"
  ],
  "author": {
    "name": "Mauri Edo (Atlassian)",
    "email": "medo@atlassian.com"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.11.4"
  },
  "homepage": "https://bitbucket.org/atlassian/openapi-diff",
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jasmine": "^2.8.8",
    "@types/js-yaml": "^3.11.2",
    "@types/lodash": "^4.14.116",
    "@types/request": "^2.47.1",
    "@types/swagger-parser": "^4.0.2",
    "@types/swagger-schema-official": "^2.0.10",
    "@types/verror": "^1.10.1",
    "conventional-changelog-lint": "1.1.9",
    "del": "^3.0.0",
    "express": "^4.16.3",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-conventional-changelog": "^1.1.24",
    "gulp-git": "^2.8.0",
    "gulp-jasmine": "^3.0.0",
    "gulp-tslint": "^8.1.2",
    "gulp-typescript": "^3.2.2",
    "jasmine": "^2.8.0",
    "minimist": "^1.2.0",
    "run-sequence": "^2.1.0",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "commander": "^2.17.1",
    "js-yaml": "^3.12.0",
    "json-schema-diff": "^0.8.2",
    "lodash": "^4.17.10",
    "openapi3-ts": "^0.6.2",
    "request": "^2.87.0",
    "swagger-parser": "5.0.2",
    "verror": "^1.10.0"
  },
  "types": "dist/api-types.d.ts"
}
